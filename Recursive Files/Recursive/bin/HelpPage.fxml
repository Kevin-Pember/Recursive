                        <TextArea id="helpMainCacTextLabel" editable="false" layoutY="997.0" prefHeight="57.0" prefWidth="310.0" style="-fx-background-radius: 0;" text="Otherwise noted as d→f, this function converts a decimal to a fraction" wrapText="true" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
                        <Label id="helpMainCacSubTitleLabel" layoutX="1.0" layoutY="1054.0" prefHeight="48.0" prefWidth="294.0" style="-fx-padding: 10;" text="Delete history button" textAlignment="RIGHT" textFill="WHITE" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
